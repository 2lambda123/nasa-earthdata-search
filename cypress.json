{
  "nodeVersion": "system",
  "baseUrl": "http://localhost:8080",
  "viewportWidth": 1400,
  "viewportHeight": 900,
  "testFiles": "**/*_spec.js",
  "fixturesFolder": "cypress/fixtures",
  "chromeWebSecurity": false,
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "threshold": 0.01
      }
    }
  }
}
